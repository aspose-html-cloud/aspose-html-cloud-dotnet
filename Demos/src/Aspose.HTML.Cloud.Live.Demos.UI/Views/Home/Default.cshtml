
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>Build HTML Converter & Renderer via C# REST SDK</h1>
				<h4>C# Cloud SDK to help you render & convert HTML documents stored on the cloud.</h4>
			</div>
			<div class="col-md-4 tc">
				<img class="col-xs-12" src="../img/aspose_html-for-net.png" alt="Aspose.HTML Cloud" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>Aspose.HTML Cloud .NET APIs Includes</h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.HTML Cloud .NET SDK" />
					</div>
					<a href="/html/conversion"><span class="spanclass">Aspose.HTML @Model.Resources["AsposeProductFor"] Cloud<em>Conversion</em></span><em>Easily convert HTML files to PDF, Markdown, MHTML and many other popular file formats without leaving your favorite browser.</em></a>
				</div>
			</div>

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>
